<body>\par
    <h1>calculater</h1>\par
    <div class="box">\par
        <label>A:</label>\par
        <input type="number" id="valuea"><br><br>\par
        <select name="operatore" id="opt">\par
            <option value="+">+</option>\par
            <option value="-">-</option>\par
            <option value="*">*</option>\par
            <option value="/">/</option>\par
            <option value="%">%</option>\par
        </select><br><br>\par
        <label>B:</label>\par
        <input type="number" id="valueb"><br><br>\par
        <button type="button" id="btn" onclick="clc()">=</button><br><br>\par
        <label for="">Result : </label>\par
        <input id="result">\par
    </div>\par
\par
    <script>\par
\par
        function clc()\par
        \{\par
           var a=parseFloat( document.getElementById("valuea").value);\par
           var b=parseFloat( document.getElementById("valueb").value);\par
           var oprt=document.getElementById("opt").value;\par
\par
if (oprt=="+")\{\par
    document.getElementById('result').value = a + b;\par
\}\par
else if (oprt =="-")\{\par
    document.getElementById('result').value = a - b;\par
\}\par
else if (oprt == "*")\{\par
    document.getElementById('result').value = a * b;\par
\}\par
else if(oprt == "/")\{\par
    document.getElementById('result').value = a / b;\par
\}\par
else\{\par
    document.getElementById('result').value = a % b;\par
\}\par
        \}\par
    </script>\par
</body>\par
</html>\par
